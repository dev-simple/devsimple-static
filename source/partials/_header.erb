<header>
    <div class="logo">
       <%= link_to svg("logo"), "/" %>
    </div>
    <nav>
        <ul>
            <li>
                <% link_to data.global.social_links.github.link do %>
                    <i class="fa fa-<%= data.global.social_links.github.icon %>"></i>
                <% end %>
            </li>
            <li>
                <% link_to data.global.social_links.youtube.link do %>
                    <i class="fa fa-<%= data.global.social_links.youtube.icon %>"></i>
                <% end %>
            </li>
        </ul>
    </nav>
</header>
